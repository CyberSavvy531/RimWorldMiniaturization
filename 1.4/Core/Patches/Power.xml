			<xpath>/Defs/ThingDef[defName = "WoodFiredGenerator"]</xpath>
			<value>
				<thingCategories>
					<li>BuildingsPower</li>
				</thingCategories>
			</value>
		</nomatch>
		<match Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "WoodFiredGenerator"]/thingCategories</xpath>
			<value>
				<li>BuildingsPower</li>
			</value>
		</match>
	</Operation>

	<Operation Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "WoodFiredGenerator"]/statBases/Mass</xpath>
